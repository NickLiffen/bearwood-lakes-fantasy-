{
  "name": "bearwood-lakes-fantasy",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "db:seed": "npx tsx scripts/seed.ts",
    "db:seed-golfers": "npx tsx scripts/seed-golfers.ts",
    "db:seed-comprehensive": "npx tsx scripts/seed-comprehensive.ts",
    "db:seed-csv": "npx tsx scripts/seed-from-csv.ts",
    "db:update-prices": "npx tsx scripts/update-prices.ts",
    "db:delete": "npx tsx scripts/delete.ts",
    "db:backup": "npx tsx scripts/backup.ts",
    "db:restore": "npx tsx scripts/restore.ts"
  },
  "dependencies": {
    "ioredis": "^5.9.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.1",
    "twilio": "^5.12.2",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@netlify/functions": "^3.1.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.3.3",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^4.0.18",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.6.1",
    "eslint": "^9.28.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^17.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.16.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^6.3.5",
    "vitest": "^4.0.18"
  }
}
